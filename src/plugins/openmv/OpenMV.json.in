    ],
    \"Category\" : \"Qt Creator\",
    \"Description\" : \"OpenMV Plugin\",
    \"Url\" : \"https://openmv.io\",
    \"Arguments\" : [
        {
            \"Name\" : \"-update_resources\",
            \"Description\" : \"Force an update of user resources from system resources\"
        },
        {
            \"Name\" : \"-open_serial_terminal\",
            \"Parameter\" : \"port_name:baud_rate\",
            \"Description\" : \"Open a serial terminal window\"
        },
        {
            \"Name\" : \"-open_udp_client_terminal\",
            \"Parameter\" : \"host_name:port\",
            \"Description\" : \"Open a UDP client terminal window\"
        },
        {
            \"Name\" : \"-open_udp_server_terminal\",
            \"Parameter\" : \"port\",
            \"Description\" : \"Open a UDP server terminal window (set port == 0 for any port)\"
        },
        {
            \"Name\" : \"-open_tcp_client_terminal\",
            \"Parameter\" : \"host_name:port\",
            \"Description\" : \"Open a TCP client terminal window\"
        },
        {
            \"Name\" : \"-open_tcp_server_terminal\",
            \"Parameter\" : \"port\",
            \"Description\" : \"Open a TCP server terminal window (set port == 0 for any port)\"
        },
        {
            \"Name\" : \"-override_read_timeout\",
            \"Parameter\" : \"time_in_ms\",
            \"Description\" : \"Ms to wait for a response (default 10000)\"
        },
        {
            \"Name\" : \"-override_read_stall_timeout\",
            \"Parameter\" : \"time_in_ms\",
            \"Description\" : \"Ms to wait before pinging (default 1000)\"
        },
        {
            \"Name\" : \"-form_key\",
            \"Parameter\" : \"key\",
            \"Description\" : \"OpenMV Cam Registration Key\"
        },
        {
            \"Name\" : \"-list_ports\",
            \"Description\" : \"Prints detected ports\"
        },
        {
            \"Name\" : \"-serial_number_filter\",
            \"Parameter\" : \"serial_number\",
            \"Description\" : \"Filter detected ports by this Serial Number\"
        },
        {
            \"Name\" : \"-auto_connect\",
            \"Description\" : \"Automatically connect to the first detected port\"
        },
        {
            \"Name\" : \"-auto_update_on_connect\",
            \"Parameter\" : \"release|development|path\",
            \"Description\" : \"Automatically update the OpenMV Cam on connect\"
        },
        {
            \"Name\" : \"-auto_erase_on_connect\",
            \"Description\" : \"Automatically erase the onboard flash on connect\"
        },
        {
            \"Name\" : \"-auto_run_on_connect\",
            \"Description\" : \"Automatically run the open script on connect\"
        },
        {
            \"Name\" : \"-full_screen\",
            \"Description\" : \"Launch in full-screen mode\"
        }
    ],
    $$dependencyList
}
